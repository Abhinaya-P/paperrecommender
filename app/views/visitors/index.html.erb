<h3>Welcome</h3>
<p><%= link_to 'Users:', users_path %> <%= User.count %> registered</p>
<hr></hr>
<h5>Analyse a paper</h5>
<%= simple_form_for(@paper) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :userdoc %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
